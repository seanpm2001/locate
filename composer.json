{
    "name": "vaersaagod/locate",
    "description": "Harness the power of the Google Autocomplete API inside Craft. Adds an autocomplete search box to Craft entries.",
    "type": "craft-plugin",
    "version": "2.2.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "locate",
        "map",
        "google-maps",
        "proximity-search",
        "address",
        "locations"
    ],
    "support": {
        "docs": "",
        "issues": ""
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Isaac Gray",
            "homepage": "https://www.vaersaagod.no/"
        }
    ],
    "require": {
        "php": "^7.0",
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "vaersaagod\\locate\\": "src/"
        }
    },
    "extra": {
        "name": "Locate",
        "handle": "locate",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "",
        "class": "vaersaagod\\locate\\Locate"
    }
}
